"RunInvalidSignatures"=dword:00000001

#禁止IE自动安装组件                      
[HKEY_LOCAL_MACHINE\SOFTWARE\Policies\Microsoft\Internet Explorer\Infodelivery\Restrictions] 
"NoJITSetup"=dword:00000001

#禁止IE检查默认浏览器设置，位数为no不检查，为yes每次启动检查，如果设置为yes则又会导致QQ农场覆盖问题。
[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\Main]
"Check_Associations"="no"

#启用快速导航选项卡，尾数0为不启用，1为启用。
[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\TabbedBrowsing]
"QuickTabsThreshold"=dword:00000000

#启用选项卡浏览，尾数0为不启用，1为启用。
[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\TabbedBrowsing]
"Enabled"=dword:00000000

#关闭多个选项卡时给出警告，尾数0为不给警告，1为给出警告(建议不给警告)。
[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\TabbedBrowsing]
"WarnOnClose"=dword:00000000

#关闭IE8第一次启动时的设置向导
[HKEY_CURRENT_USER\Software\Policies\Microsoft\Internet Explorer\Main]
"DisableFirstRunCustomize"=dword:00000002

#跳过 IE7/IE8 首次运行自定义设置
[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\Main]
"RunOnceHasShown"=dword:00000001
"RunOnceComplete"=dword:00000001
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Internet Explorer\Main]
"DisableFirstRunCustomize"=dword:00000001

