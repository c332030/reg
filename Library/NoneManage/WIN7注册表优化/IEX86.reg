[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\CommandBar]
"CommandBarEnabled"=dword:00000001

#启用选项卡分组，尾数1显示小星星和收藏夹文字描述，2只显示小星星。
[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\CommandBar]
"TextOption"=dword:00000001

#IE10隐藏收藏夹
[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\MINIE]
"LinksBandEnabled"=dword:00000000

#隐藏IE8收藏夹小星星，0为显示，1为不显示
[HKEY_CURRENT_USER\Software\Policies\Microsoft\Internet Explorer\Toolbars\Restrictions]
"NoCommandBar"=dword:00000000

#锁定IE工具栏
[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\Toolbar]
"Locked"=dword:00000001

#显示状态栏，0为不显示，1为显示
[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\MINIE]
"ShowStatusBar"=dword:00000001

[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\Download]
