[HKEY_LOCAL_MACHINE\SOFTWARE\Policies\Microsoft\Internet Explorer\Infodelivery\Restrictions]
"NoSearchBox"=dword:00000001

#禁止显示收藏夹栏，0为不显示，1为显示
[HKEY_CURRENT_USER\Software\Microsoft\Internet Explorer\LinksBar]
